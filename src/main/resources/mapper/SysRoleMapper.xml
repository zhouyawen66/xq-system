<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cnaidun.police.mapper.SysRoleMapper">


    <select id="getAllRole"  resultType="com.cnaidun.police.entity.SysRole">
        select
        s.id id,
        s.role_name roleName
        from sys_role s where s.status=1
    </select>



</mapper>